<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.kanyun.cpa.model.system.CpaRole" table="cpa_role" schema="cpa">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="roleName">
            <column name="role_name" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <set name="rolePermissions" inverse="true">
            <key>
                <column name="role_id">
                    <comment>角色ID</comment>
                </column>
            </key>
            <one-to-many class="cn.kanyun.cpa.model.system.RolePermission" />
        </set>
        <set name="userRole" inverse="true">
            <key>
                <column name="role_id">
                    <comment>角色ID</comment>
                </column>
            </key>
            <one-to-many class="cn.kanyun.cpa.model.system.UserRole" />
        </set>
    </class>
</hibernate-mapping>