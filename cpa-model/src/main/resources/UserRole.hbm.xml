<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.kanyun.cpa.model.system.UserRole" table="user_role" schema="cpa">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="roleId">
            <column name="role_id" sql-type="int(11)" not-null="true"/>
        </property>
        <many-to-one name="cpaUser" class="cn.kanyun.cpa.model.user.CpaUser" fetch="select">
            <column name="user_id">
                <comment>用户ID</comment>
            </column>
        </many-to-one>
        <many-to-one name="cpaRole" class="cn.kanyun.cpa.model.system.CpaRole" fetch="select">
            <column name="role_id">
                <comment>角色ID</comment>
            </column>
        </many-to-one>
    </class>
</hibernate-mapping>